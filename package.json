{
  "name": "mpvuebase",
  "version": "1.0.0",
  "description": "基于 mpvue的起始项目",
  "author": "Mr.Ypeng",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "dev:proxy1": "cross-env METHOD=proxy1 npm-run-all --parallel dev proxy1",
    "dev:proxy2": "cross-env METHOD=proxy2 npm-run-all --parallel dev proxy2",
    "dev:nginx": "cross-env METHOD=nginx npm run dev",
    "dev:server": "cross-env METHOD=server npm-run-all --parallel dev server",
    "start": "npm run dev:proxy1",
    "build": "node build/build.js",
    "lint": "eslint --ext .js,.vue src",
    "proxy1": "node server/app.js",
    "proxy2": " node server/proxy.js",
    "server": "nodemon server/server/index.js --exec babel-node --presets es2015,stage-2",
    "filemap": "filemap -i node_modules build config dist .git"
  },
  "dependencies": {
    "flyio": "^0.4.5",
    "mpvue": "^1.0.6",
    "vuex": "^2.5.0"
  },
